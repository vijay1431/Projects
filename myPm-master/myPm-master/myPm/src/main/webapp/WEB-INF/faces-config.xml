<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<application>
		<resource-bundle>
			<base-name>com.com3g.myPm.i18n.message.ErrorMessages</base-name>
			<var>errorMsg</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>com.com3g.myPm.i18n.message.InfoMessages</base-name>
			<var>infoMsg</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>com.com3g.myPm.i18n.message.ValidationMessages</base-name>
			<var>valMsg</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>com.com3g.myPm.i18n.message.url</base-name>
			<var>url</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>com.com3g.myPm.i18n.message.labels</base-name>
			<var>lbl</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>com.com3g.myPm.i18n.message.messages</base-name>
			<var>msg</var>
		</resource-bundle>
		<!-- Default local language -->
		<locale-config>
			<default-locale>fr</default-locale>
		</locale-config>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>
</faces-config>
